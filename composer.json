{
    "name": "mcms/core-package",
    "type": "library",
    "description": "The core of the CMS",
    "keywords": [
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Bouclas",
            "email": "mbouclas@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "teepluss/theme": "^2.0",
        "doctrine/dbal": "^2.5",
        "barryvdh/laravel-debugbar": "^2.2",
        "cocur/slugify": "^2.1",
        "themsaid/laravel-multilingual": "^1.0",
        "arrilot/laravel-widgets": "^3.3",
        "laracasts/utilities": "~2.0",
        "mcamara/laravel-localization": "1.2.*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "barryvdh/laravel-translation-manager": "0.2.x",
        "kalnoy/nestedset": "^4.1",
        "rtconner/laravel-tagging": "~2.1",
        "santigarcor/laratrust": "3.0.x",
        "rtconner/laravel-likeable": "~1.2",
        "torann/geoip": "^1.0",
        "geoip2/geoip2": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Mcms\\Core\\": "src"
        },
        "psr-0": {
            "Mcms\\Core": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mcms\\Core\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}